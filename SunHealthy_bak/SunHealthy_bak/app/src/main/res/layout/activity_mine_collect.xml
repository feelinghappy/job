<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mine_collect"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_color"
    android:orientation="vertical"
    tools:context="com.longcai.medical.ui.activity.MineCollectActivity">


    <include layout="@layout/title_bar"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/mine_collect_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/title_position"></android.support.v7.widget.RecyclerView>
</LinearLayout>
