                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="119px"
                    android:text="@string/new_setting_tx1"
                    android:textColor="@color/new_setting_tx_color"
                    android:textSize="29px" />

                <ImageView
