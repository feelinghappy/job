<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/menu_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="188px"
        android:layout_height="176px"
        android:layout_marginRight="20px"
        android:background="@mipmap/fammily_menu"
        android:gravity="center"
        android:layout_alignParentRight="true"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/menu_family_manage"
            android:layout_width="match_parent"
            android:layout_height="82px"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="28px"
                android:layout_height="28px"
                android:src="@mipmap/icon_family_manage" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/manage_family"
                android:layout_marginLeft="6px"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_30"/>

        </com.zhy.autolayout.AutoLinearLayout>

        <View
            android:id="@+id/horizontal_line_top"
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#eeeeee"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/menu_quit_family"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="76px"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="28px"
                android:layout_height="28px"
                android:src="@mipmap/icon_family_quit" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/quit_family"
                android:layout_marginLeft="6px"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_30"/>

        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>
